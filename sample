from BLHtoENU import trans_coordinate 
if __name__ == '__main__':

    instance = trans_coordinate("sakai") #instance name
    #34째34'00.0"N 135째29'00.0"E
    lon = 1352900.0 #ddmmss 135째29'00"
    lat = 343400.0 #ddmmss 34째34'00"
